{
  "name": "h5",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "start:test": "export API_ENV='test' && node scripts/start.js",
    "start:uat": "export API_ENV='uat' && node scripts/start.js",
    "build": "npm run clean && node scripts/build.js",
    "test": "node scripts/test.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "clean": "rimraf ./build",
    "lint": "eslint --fix --ext .js,.jsx src config",
    "dev": "cross-env NODE_ENV=dev nodemon app.js",
    "deploy": "cross-env NODE_ENV=production pm2 start",
    "deploy:docker": "sh deploy.sh",
    "docker-compose:up": "docker-compose up -d"
  },
  "dependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/plugin-transform-react-jsx-self": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@svgr/webpack": "4.1.0",
    "antd-mobile": "^2.3.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-import": "^1.12.2",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.2",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "classnames": "^2.2.6",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "http-proxy-middleware": "^0.19.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "js-cookie": "^2.2.1",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-px-to-viewport": "^1.1.0",
    "postcss-safe-parser": "4.0.1",
    "postcss-write-svg": "^3.0.1",
    "react": "^16.8.4",
    "react-app-polyfill": "^0.2.2",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.8.4",
    "react-router": "^5.1.2",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "resolve": "1.10.0",
    "sass-loader": "7.1.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "source-map-explorer": "^2.1.1",
    "style-loader": "0.23.1",
    "styled-components": "^4.4.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "/Users/rrd/Evolut/admin/evolut-admin/node_modules/jest-watch-typeahead/filename.js",
      "/Users/rrd/Evolut/admin/evolut-admin/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "devDependencies": {
    "chalk": "^4.1.2",
    "cross-env": "^6.0.3",
    "fundebug-javascript": "^2.0.0",
    "fundebug-revideo": "^0.6.1",
    "nodemon": "^1.19.4",
    "rimraf": "^2.6.3",
    "speed-measure-webpack-plugin": "^1.3.3",
    "typescript": "^4.6.3"
  }
}
